{
  "name": "unlock-auto",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "db:gen:global": "drizzle-kit generate --config=drizzle.global.config.ts",
    "db:migrate:global": "drizzle-kit migrate --config=drizzle.global.config.ts",
    "db:gen:tenant": "drizzle-kit generate --config=drizzle.tenant.config.ts",
    "db:migrate:tenant:one": "node scripts/migrate-tenant.js",
    "db:studio": "drizzle-kit studio --config=drizzle.global.config.ts",
    "auth:gen": "better-auth generate",
    "docker:up": "docker compose -f docker/docker-compose.local.yml --env-file ./.env up",
    "docker:down": "docker compose -f docker/docker-compose.local.yml down",
    "docker:logs": "docker compose -f docker/docker-compose.local.yml logs -f",
    "setup:pgbouncer": "bash scripts/setup-pgbouncer-userlist.sh",
    "setup:all": "npm run setup:pgbouncer && npm run db:gen:global && npm run db:gen:tenant"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.30",
